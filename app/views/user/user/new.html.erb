<h1>Sign up</h1>
<%= form_for :user, url: user_user_index_path do |f| %>
    <%= f.label :user_name %><br>
    <%= f.text_field :user_name %><br>
    <%= f.label :email %><br>
    <%= f.text_field :email %><br>
    <%= f.label :password %><br>
    <%= f.password_field :password %><br>
    <%= f.submit %>
<% end %>