<%= stylesheet_link_tag 'admin', media: 'all', 'data-turbolinks-track': 'reload' %>
<h1>Sign up</h1>
<%= form_for :admin, url: admin_admin_index_path do |f| %>
    <%= f.label :user_name %><br>
    <%= f.text_field :user_name %><br>
    <%= f.label :password %><br>
    <%= f.password_field :password, id: 'user_user_name'%><br>
    <%= f.submit %>
<% end %>